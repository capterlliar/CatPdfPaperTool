<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TreeItem?>
<?import javafx.scene.layout.HBox?>
<?import org.kordamp.ikonli.javafx.FontIcon?>
<?import javafx.scene.control.TextArea?>
<fx:root type="TreeItem"
         fx:id="fileNameOption"
         xmlns:fx="http://javafx.com/fxml">
    <value>
        <HBox fx:id="value" stylesheets="@FilenameOption.css" spacing="5">
            <TextArea prefWidth="200" wrapText="true" prefRowCount="3" fx:id="textArea"/>
            <Button onAction="#exchange">
                <graphic>
                    <FontIcon iconLiteral="fa-exchange"/>
                </graphic>
            </Button>
        </HBox>
    </value>
</fx:root>
